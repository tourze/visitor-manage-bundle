{
  "name": "tourze/visitor-manage-bundle",
  "description": "Comprehensive visitor management bundle for Symfony applications with registration, approval workflow, digital passes, and audit logging",
  "type": "symfony-bundle",
  "license": "MIT",
  "require": {
    "doctrine/dbal": "^4.0",
    "doctrine/doctrine-bundle": "^2.13",
    "doctrine/orm": "^3.0",
    "easycorp/easyadmin-bundle": "^4",
    "knplabs/knp-menu": "^3.7",
    "symfony/config": "^7.3",
    "symfony/dependency-injection": "^7.3",
    "symfony/doctrine-bridge": "^7.3",
    "symfony/event-dispatcher": "^7.3",
    "symfony/framework-bundle": "^7.3",
    "symfony/http-foundation": "^7.3",
    "symfony/http-kernel": "^7.3",
    "symfony/routing": "^7.3",
    "symfony/validator": "^7.3",
    "symfony/yaml": "^7.3",
    "tourze/bundle-dependency": "1.*",
    "tourze/doctrine-indexed-bundle": "0.0.*",
    "tourze/doctrine-timestamp-bundle": "0.0.*",
    "tourze/easy-admin-menu-bundle": "0.1.*",
    "tourze/enum-extra": "0.1.*",
    "tourze/symfony-dependency-service-loader": "0.0.*",
    "tourze/symfony-routing-auto-loader-bundle": "0.0.*"
  },
  "require-dev": {
    "doctrine/data-fixtures": "^2.0",
    "doctrine/doctrine-fixtures-bundle": "^4.0",
    "doctrine/persistence": "^4.1",
    "phpstan/phpstan": "^2.1",
    "phpunit/phpunit": "^11.5",
    "tourze/phpunit-base": "1.*",
    "tourze/phpunit-doctrine-entity": "0.0.*",
    "tourze/phpunit-enum": "0.0.*",
    "tourze/phpunit-symfony-kernel-test": "0.0.*",
    "tourze/phpunit-symfony-unit-test": "0.0.*",
    "tourze/phpunit-symfony-web-test": "0.0.*",
    "tourze/symfony-testing-framework": "0.0.*"
  },
  "autoload": {
    "psr-4": {
      "Tourze\\VisitorManageBundle\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tourze\\VisitorManageBundle\\Tests\\": "tests"
    }
  }
}